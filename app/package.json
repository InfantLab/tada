{
  "name": "tada",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "description": "Personal lifelogger - Track Activities, Discover Achievements",
  "scripts": {
    "dev": "DATABASE_URL=file:/workspaces/tada/app/data/db.sqlite nuxt dev",
    "build": "nuxt build",
    "build:docker": "NUXT_TYPESCRIPT_TYPECHECK=false nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "verify-tests": "bash scripts/verify-tests.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@vitest/coverage-v8": "^4.0.17",
    "drizzle-orm": "^0.38.3",
    "emoji-picker-element": "^1.28.1",
    "lucia": "^3.2.2",
    "nanoid": "^5.1.6",
    "oslo": "^1.2.1",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint": "^0.7.4",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@types/bun": "^1.1.14",
    "@types/papaparse": "^5.5.2",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vitest/ui": "^3.0.0",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "happy-dom": "^16.0.0",
    "nuxt": "^3.15.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0",
    "vue-tsc": "^3.2.2"
  }
}
